{
  "compact": false,
  "presets": [
    [
      "latest",
      {
        "modules": false
      }
    ],
    "babel-preset-stage-0",
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}